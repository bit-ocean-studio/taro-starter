{
  "name": "taro-stater",
  "description": "⚛️ An opinionated Taro starter template.",
  "version": "1.0.0",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "staging:weapp": "taro build --type weapp --mode staging",
    "staging:swan": "taro build --type swan --mode staging",
    "staging:alipay": "taro build --type alipay --mode staging",
    "staging:tt": "taro build --type tt --mode staging",
    "staging:h5": "taro build --type h5 --mode staging",
    "staging:rn": "taro build --type rn --mode staging",
    "staging:qq": "taro build --type qq --mode staging",
    "staging:jd": "taro build --type jd --mode staging",
    "staging:quickapp": "taro build --type quickapp --mode staging",
    "postinstall": "weapp-tw patch"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.5",
    "@tarojs/components": "3.6.30",
    "@tarojs/helper": "3.6.30",
    "@tarojs/plugin-framework-react": "3.6.30",
    "@tarojs/plugin-platform-alipay": "3.6.30",
    "@tarojs/plugin-platform-h5": "3.6.30",
    "@tarojs/plugin-platform-jd": "3.6.30",
    "@tarojs/plugin-platform-qq": "3.6.30",
    "@tarojs/plugin-platform-swan": "3.6.30",
    "@tarojs/plugin-platform-tt": "3.6.30",
    "@tarojs/plugin-platform-weapp": "3.6.30",
    "@tarojs/react": "3.6.30",
    "@tarojs/runtime": "3.6.30",
    "@tarojs/shared": "3.6.30",
    "@tarojs/taro": "3.6.30",
    "lodash": "4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "taro-ui": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@bit-ocean/cli": "^0.0.19",
    "@bit-ocean/prettier-config": "^0.0.19",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.13",
    "@tarojs/cli": "3.6.30",
    "@tarojs/taro-loader": "3.6.30",
    "@tarojs/webpack5-runner": "3.6.30",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "autoprefixer": "^10.4.19",
    "babel-preset-taro": "3.6.30",
    "eslint": "^8.57.0",
    "eslint-config-taro": "3.6.30",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.38",
    "postcss-rem-to-responsive-pixel": "^6.0.1",
    "prettier": "^3.2.5",
    "react-refresh": "^0.14.2",
    "stylelint": "16.5.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "weapp-tailwindcss": "^3.2.1",
    "webpack": "^5.91.0"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "private": true,
  "license": "MIT"
}
